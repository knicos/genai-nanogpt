{
    "name": "@genai-fi/nanogpt",
    "version": "0.0.0",
    "type": "module",
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/main.js"
    },
    "repository": {
        "url": "https://github.com/knicos/genai-nanogpt.git",
        "type": "git"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:script": "tsc -p scripts/tsconfig.json",
        "preview": "vite preview",
        "lint": "eslint",
        "test": "vitest",
        "ci:test": "vitest --coverage --reporter=junit --outputFile=junit.xml",
        "coverage": "vitest run --coverage",
        "train": "tsx scripts/train.ts --batch 32",
        "generate": "tsx scripts/generate.ts",
        "evaluate": "tsx scripts/evaluate.ts",
        "debug": "tsx scripts/debug.ts"
    },
    "peerDependencies": {
        "@tensorflow/tfjs": "^4.22.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "@tensorflow/tfjs-node-gpu": "^4.22.0",
        "@types/node": "^22.13.14",
        "@types/papaparse": "^5.3.16",
        "@types/yargs": "^17.0.33",
        "@vitest/coverage-v8": "^3.2.4",
        "chalk": "^5.4.1",
        "dayjs": "^1.11.13",
        "eslint": "^9.32.0",
        "glob": "^11.0.3",
        "jsdom": "^26.1.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0",
        "vite": "^7.0.6",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.2.4",
        "yargs": "^18.0.0"
    },
    "dependencies": {
        "@dsnp/parquetjs": "^1.8.7",
        "eventemitter3": "^5.0.1",
        "jszip": "^3.10.1",
        "papaparse": "^5.5.3"
    }
}
